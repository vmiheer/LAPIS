{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "default",
      "displayName": "default",
      "description": "Default preset",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "installDir": "${sourceDir}/install",
      "cacheVariables": {
        "LLVM_BUILD_EXAMPLES": "ON",
        "LLVM_ENABLE_PROJECTS": "mlir",
        "LLVM_TARGETS_TO_BUILD": "X86",
        "LLVM_ENABLE_ASSERTIONS": "ON",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "LLVM_USE_LINKER": "mold",
        "LLVM_ENABLE_LLD": "OFF",
        "LLVM_PARALLEL_COMPILE_JOBS": "24",
        "LLVM_PARALLEL_LINK_JOBS": "2",
        "CMAKE_C_COMPILER_LAUNCHER": "ccache",
        "CMAKE_CXX_COMPILER_LAUNCHER": "ccache",
        "LLVM_CCACHE_BUILD": "ON",
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "notchpeak",
      "displayName": "notchpeak",
      "description": "Default preset for notchpeak",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "installDir": "${sourceDir}/../../llvm-install",
      "cacheVariables": {
        "LLVM_ENABLE_PROJECTS": "mlir",
        "LLVM_TARGETS_TO_BUILD": "X86",
        "LLVM_ENABLE_ASSERTIONS": "ON",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "LLVM_ENABLE_LLD": "ON",
        "LLVM_PARALLEL_COMPILE_JOBS": "24",
        "LLVM_PARALLEL_LINK_JOBS": "8",
        "CMAKE_C_COMPILER_LAUNCHER": "ccache",
        "CMAKE_CXX_COMPILER_LAUNCHER": "ccache",
        "LLVM_CCACHE_BUILD": "ON",
        "CMAKE_BUILD_TYPE": "Release",
        "MLIR_ENABLE_BINDINGS_PYTHON": "ON"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "base",
      "displayName": "base",
      "description": "base",
      "configurePreset": "default",
      "targets": [
        "lib/libmlir_c_runner_utils.so",
        "mlir-opt",
        "mlir-translate",
        "libmlir_runner_utils.so",
        "llc",
        "opt",
        "llvm-symbolizer"
      ]
    },
    {
      "name": "default",
      "inherits": "base",
      "displayName": "Default",
      "description": "Default preset",
      "configurePreset": "default"
    },
    {
      "name": "notchpeak",
      "inherits": "base",
      "displayName": "notchpeak",
      "description": "Default preset for notchpeak",
      "configurePreset": "notchpeak"
    }
  ],
  "workflowPresets": [
    {
      "name": "default",
      "description": "default",
      "steps": [
        {
          "type": "configure",
          "name": "default"
        },
        {
          "type": "build",
          "name": "default"
        }
      ]
    },
    {
      "name": "notchpeak",
      "description": "default workflow for notchpeak",
      "steps": [
        {
          "type": "configure",
          "name": "notchpeak"
        },
        {
          "type": "build",
          "name": "notchpeak"
        }
      ]
    }
  ]
}
